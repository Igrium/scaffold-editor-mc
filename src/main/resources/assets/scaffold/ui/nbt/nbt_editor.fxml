<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="rootPane" maxHeight="-Infinity" prefHeight="500.0" prefWidth="300.0" stylesheets="@../css/scaffold.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scaffoldeditor.editormc.nbt_browser.NBTEditorController">
   <bottom>
      <ButtonBar fx:id="buttonBar" buttonOrder="" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="editButton" disable="true" mnemonicParsing="false" onAction="#editEntry" text="Edit" />
            <Button defaultButton="true" mnemonicParsing="false" onAction="#apply" text="Apply" />
        </buttons>
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" />
         </BorderPane.margin>
      </ButtonBar>
   </bottom>
</BorderPane>
