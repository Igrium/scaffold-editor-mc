<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@css/scaffold.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scaffoldeditor.editormc.ui.controllers.FXMLCompileProgressController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
        </buttons>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ButtonBar>
   </bottom>
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Compiling level...">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <ProgressBar fx:id="progressBar" maxWidth="100000.0" progress="0.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="3.0" />
         </BorderPane.margin>
      </VBox>
   </top>
   <center>
      <TextArea fx:id="outputField" editable="false" prefHeight="200.0" prefWidth="200.0" styleClass="debug-text" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
